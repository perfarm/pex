{
  "name": "pex",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "deploy": "yarn build && yarn gcp-predeploy && gcloud app deploy",
    "gcp-predeploy": "find ./build -name '\\[...*' -exec bash -c 'mv \"$1\" \"${1/.../@@@}\"' -- {} \\;",
    "gcp-build": "find ./build -name '\\[@@@*' -exec bash -c 'mv \"$1\" \"${1/@@@/...}\"' -- {} \\;",
    "start": "next start -p 8080",
    "start:dev": "next dev",
    "start:prod": "next start",
    "lint": "next lint",
    "type:check": "tsc --noEmit --skipLibCheck"
  },
  "engines": {
    "node": ">=16.14.0 <=18.16.0"
  },
  "dependencies": {
    "@radix-ui/react-switch": "1.0.3",
    "@radix-ui/react-toggle-group": "1.0.4",
    "@radix-ui/react-tooltip": "1.0.4",
    "@stitches/react": "1.2.8",
    "@types/node": "20.5.7",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "antd": "5.9.0",
    "axios": "1.4.0",
    "beautiful-react-hooks": "5.0.0",
    "cpf-cnpj-validator": "1.0.3",
    "dotenv": "^16.3.1",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "firebase": "10.3.0",
    "firebase-admin": "11.10.1",
    "jsonwebtoken": "9.0.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "next": "13.2.4",
    "next-pwa": "5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "react-select": "5.7.4",
    "rsuite": "5.38.0",
    "rsuite-table": "5.11.1",
    "slugify": "1.6.6",
    "socket.io": "4.7.2",
    "socket.io-client": "4.7.2",
    "socket.io-react-hook": "2.4.1",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@google-cloud/appengine-admin": "3.0.0",
    "@types/next-pwa": "5.6.4"
  }
}
